.container-fluid
  .col-md-4
  .col-md-3
    .row
      = render 'layouts/form'
  .col-md-4
  .col-md-12
    .row
      %ul
        - @tweets.each do |tweet|
          - unless tweet.include?("RT")
            -# - tweet = tweet.sub(';','')
            - @urls = URI.extract(tweet)
            - @urls.each do |url|
              - if url.include?('http')
                - link = url.sub(';','')
                .row
                  = tweet
                  = link_to "#{link}",  "#{link}", target: "_"
                .line





